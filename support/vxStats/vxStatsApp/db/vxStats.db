	record(ai,"$(IOC):cpuUsedPercent") {
	    field(SCAN,"I/O Intr")
	    field(DTYP,"VX stats")
	    field(INP,"@cpu")
	    field(HIHI,"90.0")
	    field(HIGH,"60.0")
	    field(HHSV,"MAJOR")
	    field(HSV,"MINOR")
	    field(EGU,"%")
	}
	
